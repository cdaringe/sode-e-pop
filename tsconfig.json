{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "esModuleInterop": false,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "types": ["typed-factorio/runtime", "typed-factorio/strict-index-types"]
  },
  "tstl": {
    "luaTarget": "5.2",
    "noImplicitSelf": true,
    "noHeader": true,
    "luaLibImport": "require-minimal"
  }
}
